<UserControl
    x:Class="BringMeBack.UserControls.MenuUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">


    <UserControl.Resources>
        <Style x:Key="Menu" TargetType="Rectangle">
            <Setter Property="Fill" Value="#00A6FF"/>
        </Style>
    </UserControl.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>


        <Rectangle x:Name="rect"  Fill="Red" Height="100" Grid.Column="0" Grid.Row="0"  VerticalAlignment="Top"/>
        <Button Background="Transparent" Style="{StaticResource NavigationBackButtonNormalStyle}" Grid.Row="0" Grid.Column="0" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20" Click="Retour"/>
    </Grid>

</UserControl>
